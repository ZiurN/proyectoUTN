<section class="info-pagina">
	<section class="foto-inicio">
		<img src="/img/modelado.jpg" alt="">
	</section>
	<div id="espacio-vacio"></div>

	<div id="btn-abajo-1" class="btn-abajo">
		<a href="#main"><span class="btn-span">Ver Proyectos</span> 
		<br> 
		<i class="fas fa-angle-double-down"></i></a>
	</div>

	<h1 class="titulo-pagina" id="titulo-pagina">modelado</h1>
</section>

<section id="main" class="main">
	<div class="sub-main-impar sub-main-margen contenedor-proyectos">	
	{{#each datos}}
		<div class="proyecto3d-contenedor">
           {{!-- INICIO DEL FOR --}}
			<div class="proyecto3d-titulo">
				<h4> {{this.Proyecto}}</h4>
			</div>
			<div class="proyecto3d-foto">
				<img src=" {{this.Portada}}" alt="">
			</div>
			<div class="proyecto3d-descripcion">
				<p> {{this.Descripcion}}</p>
			</div>
			<button class="proyecto3d-boton-imagenes btn btn-centrado btn-blanco" value="{{this.Album}}"  type="button">Ver im√°genes</button>
			<div class="m-3d-visor" name="{{this.Album}}"></div>
           {{!-- FIN DEL FORM --}}
		</div>
		{{/each}}	
		<div id='visor-album' class='visor-album transicion1'>
		
		</div>
	</div>
</section>
<script type="text/javascript" src="/js/modelado.js"></script>